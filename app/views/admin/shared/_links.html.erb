<div class="form-group text-center">
  <%- if controller_name != 'sessions' %>
    <%= link_to "ログイン", new_session_path(resource_name), :class => "btn btn-outline-primary" %><br/>
  <% end -%>
</div>
<div class="form-group text-center">
    <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
      <%= link_to "新規アカウント登録", new_registration_path(resource_name) %><br/>
    <% end -%>
</div>